{
  "name": "HealthyInstagram",
  "description": "Disable likes, comments, stories and direct messages on instagram",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/"],
      "js": ["content.js"]
    }
  ], 

  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  
  "browser_action": {
    "default_title": "HIG",
    "default_popup": "popup.html"
  } 
}